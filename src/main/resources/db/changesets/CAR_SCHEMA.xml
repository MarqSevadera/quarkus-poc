<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <changeSet id="1" author="mrq">
        <createTable tableName="APP_CAR">
            <column name="ID" type="bigint" autoIncrement="true" generationType="BY DEFAULT">
                <constraints primaryKey="true" nullable="false" primaryKeyName="PK_APP_CAR_ID"/>
            </column>
            <column name="MODEL" type="VARCHAR(50)"/>
            <column name="MAKE" type="VARCHAR(50)"/>
            <column name="YEAR" type="INTEGER"/>
        </createTable>
    </changeSet>

</databaseChangeLog>